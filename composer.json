{
    "name" : "rin-project/business-common",
    "type" : "symfony-bundle",
    "description" : "Base business common and entity",
    "keywords" : ["Symfony", "Business"],
    "homepage" : "https://github.com/dream-studio-china/business-common",
    "license" : "MIT",
    "authors" :
    [
        {
            "name" : "Rin Peng",
            "email" : "immane@163.com",
            "homepage" : "https://github.com/immane",
            "role" : "Developer"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "symfony/dotenv": "5.1.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.1.*",
        "sensio/framework-extra-bundle": "^5.5",
        "symfony/yaml": "5.1.*",
        "rin-project/fast-crud": "^0.1"
    },
    "require-dev": {
    },

    "autoload": {
        "psr-4": {
            "RinProject\\BusinessCommonBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RinProject\\BusinessCommonBundle\\Tests\\": "tests/"
        }
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
